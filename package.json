{
  "name": "@amittksharma/react-ts-sourcemaps",
  "version": "1.0.6",
  "description": "To embedd the sourcemaps of a react application and git information in an application",
  "main": "index.js",
  "scripts": {
    "start": "cls && node bin/index.js",
    "commit": "npx git-cz"
  },
  "bin": {
    "react-ts-sourcemaps-git": "bin/react-sourcemaps-git.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amittkSharma/react-ts-sourcemaps.git"
  },
  "keywords": [
    "React",
    "Typescript",
    "Start/Build process",
    "UI App Bootstrap",
    "Sourcemaps",
    "GIT information",
    "GIT Branch",
    "Commit Hash"
  ],
  "dependencies": {
    "ansi-colors": "3.2.3",
    "commander": "2.19.0",
    "git-revision-webpack-plugin": "3.0.3",
    "proxyquire": "2.1.0",
    "rewire": "4.0.1",
    "source-map-loader": "0.2.4",
    "cz-conventional-changelog": "2.1.0",
    "commitizen": "3.0.7"
  },
  "peerDependencies": {
    "react-scripts": "2.x.x",
    "react-scripts-ts": "3.x.x",
    "typescript": "3.x.x"
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.1.0",
    "react-scripts": "2.1.3",
    "react-scripts-ts": "3.1.0",
    "typescript": "3.3.1"
  },
  "author": "AMITT K SHARMA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amittkSharma/react-ts-sourcemaps/issues"
  },
  "homepage": "https://github.com/amittkSharma/react-ts-sourcemaps#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
